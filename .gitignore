# .gitignore file

# Node modules directory (if using Node.js)
node_modules/

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Dependency directories
jspm_packages/
bower_components/

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Directory for secret configuration files
config/

# Build and production files
build/
dist/
out/

# Coverage directory used by testing tools
coverage/

# IDE and editor specific files
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# MacOS specific files
.DS_Store

# Windows specific files
Thumbs.db
ehthumbs.db
Desktop.ini
$RECYCLE.BIN/

# Temporary files
*.tmp
*.temp
